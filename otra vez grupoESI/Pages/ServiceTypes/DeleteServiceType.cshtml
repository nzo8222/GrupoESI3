@page
@model GrupoESINuevo.DeleteServiceTypeModel

@{
    ViewData["Title"] = "Delete";
}
    <div class="container backgroundWhite">
        <h2 class="text-info">Borrar tipo de servicio</h2>
        <div class="card container">
            <div class="card-header bg-dark text-light ml-0 row container">
                <div class="col-6">
                    <i class="fa fa-user-o"></i>
                </div>
            </div>
            <div class="card-body">
                <div>
                    <hr />
                    <dl class="row">
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.ServiceType.Category)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.ServiceType.Category)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.ServiceType.Descripcion)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.ServiceType.Descripcion)
                        </dd>
                    </dl>

                    <form method="post">
                        <input type="hidden" asp-for="ServiceType.Id" />
                        <input type="submit" value="Delete" class="btn btn-danger" /> |
                        <a asp-page="./IndexServiceType">Regresar a la lista</a>
                    </form>
                </div>
            </div>
        </div>
    </div>

        
